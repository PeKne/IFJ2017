/' Program 1: () '/

scope 'Telo programu
	Dim a As Integer
	DIM vysl AS INTEGER

	PrinT !"Zadejte cislo";
	InpuT A
	If a < 0 THEN
		print !"\nNelze spocitat\n";
	ELSE
		Vysl = 1
		Do WHile A > 0
			VYSL = vysl * a
			a=A- 1
		LooP
		Print !"\nVysledek je:" ; vYsl ; !"\n";
	end IF
END SCOPE

dim doublevar as double
doublevar = 1.553E
