declare function get_year(number as integer, sec_num as integer) as integer

function get_year(number as integer, second as integer) as integer
    print !"Enter a year: ";
    dim year as integer
    input year
    print number;
    print second;
    return year
end function

scope
    dim year as integer
    dim num as integer = 1
    dim sec_num as integer = 2
    year = get_year(num, sec_num)
    print year;
end scope
