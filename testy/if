scope
dim a as integer
a = 4
if (a < 5) then            '1
	print !"if1\n";        '1
	if (a = 2) then        '2
		print !"if2\n";    '2
	else                   '2
		print !"else2\n";  '2
	end if                 '2 
else                       '1
print !"else1";            '1

end if                     '0
end scope
